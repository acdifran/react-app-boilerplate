{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-shadow": "off",
    "react/prop-types": 0,
    "import/no-named-as-default": 0,
    "react/jsx-one-expression-per-line": 0,
    "no-else-return": 0,
    "one-var": 0
  }
}
